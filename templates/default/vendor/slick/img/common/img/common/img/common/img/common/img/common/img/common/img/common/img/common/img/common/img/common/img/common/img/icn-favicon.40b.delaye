<script>
	$(".submit_newsletter").click(function() {
		var email = $('#email').val();

		var re = /^([a-zA-Z0-9_.-])+@([a-zA-Z0-9_.-])+\.([a-zA-Z])+([a-zA-Z])+/;
		var inemail = $("#email").val();

		if (email == "") {
			//$('.msg').text("Nhập Email");
			alert("Nhập Email");
		} else if (!re.test(inemail)) {
			//$('#msg').html("Email sai định dạng");
			alert("Email sai định dạng");
		} else {
			$.post('../../../../../../../../../../../../../../../../../../../../../../../../../../../../index_ajaxd4c7.html?module_name=footer&amp;action=subscribe', {
				email : email
			}, function(rel) {
				if (rel == "exist") {
					//$('.msg').text("Email đã tồn tại");
					alert("Email đã tồn tại");
				} else if (rel == "Successfully") {
					//$('.msg').text("Đăng ký thành công!");
					alert("Đăng ký thành công");

					document.getElementById("email").value = "";
				}

			});
		}
	}); 
</script>



