<script>
    $(document).ready(function () {
        function MNight(more) {
            var link = $("a[href='https://app-apac.thebookingbutton.com/properties/imperialvtdirect']");
            if(link.length > 0){
                var href = link.attr("href");
                href = href + more;
                link.attr("href", href);
            }
        }
        if(window.more == undefined){
            let begin = new Date();
            let end = new Date(begin);
            end.setDate(begin.getDate() + 1);
            let beginStr = begin.toLocaleString('en-us', {year: 'numeric', month: '2-digit', day: '2-digit'}).
            replace(/(\d+)\/(\d+)\/(\d+)/, '$2-$1-$3');
            let endStr = end.toLocaleString('en-us', {year: 'numeric', month: '2-digit', day: '2-digit'}).
            replace(/(\d+)\/(\d+)\/(\d+)/, '$2-$1-$3');
            var more = '?number_adults=2&currency=VND&check_in_date=' + beginStr +'&check_out_date=' + endStr;
            MNight(more);
        }
    });
